<%- include('partials/header') -%>
<h1>Home</h1>
<p><%=homeCont%></p>

<% posts.forEach(function(post){ %>
    <form action="/post" method="get">
        <h1><%=post.title%></h1>
        <p>
            <%=post.content.substring(0,100) + "..."%>
            <a href="/posts/<%=post.title%>">Read More</a>
        </p>
    </form>
<% }) %> 

<a href="/compose">COMPOSE</a>

<%- include('partials/footer') -%>
